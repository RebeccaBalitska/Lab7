import random

n = int(input("Введіть розмірність матриці: "))

matrix = [[random.randint(-10, 10) for z in range(n)] for i in range(n)]

print("-" * 5, "Початкова матриця", "-" * 5)
for i in matrix:
    print(i)

for el in matrix[::2]:
    el.sort()

print("-" * 5, "Кінцева матриця", "-" * 5)
for i in matrix:
    print(i)

	
